<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybaby.android_final_project" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.AddNewControlActivity"
            android:label="@string/title_activity_add_new_control" >
        </activity>
        <activity
            android:name=".Activities.ProgressActivity"
            android:label="@string/title_activity_progress" >
        </activity>
        <activity
            android:name=".Activities.PerfilActivity"
            android:label="@string/title_activity_perfil" >
        </activity>
        <activity
            android:name=".Activities.ControlHistoryActivity"
            android:label="@string/title_activity_history_control" >
        </activity>
    </application>

</manifest>
